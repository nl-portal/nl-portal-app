{
  "name": "nl-portal-app",
  "version": "3.0.0",
  "author": "Ritense",
  "packageManager": "pnpm@10.18.0",
  "license": "EUPL-1.2",
  "type": "module",
  "engines": {
    "node": "22",
    "pnpm": "^10"
  },
  "scripts": {
    "dev": "vite --port 3000 --mode development",
    "build": "tsc && vite build",
    "preinstall": "npx only-allow pnpm",
    "preview": "vite preview --port 3000",
    "generate-pwa-assets": "pwa-asset-generator ./src/assets/pwa-logo.svg ./public/pwa --favicon --mstile --opaque  false --maskable true --index  index.html --manifest ./public/manifest.json --path-override '../pwa'",
    "clean": "rimraf build",
    "lint": "eslint .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@apollo/client": "3.13.8",
    "@gemeente-denhaag/design-tokens-components": "5.1.0",
    "@gemeente-denhaag/icons": "4.0.0",
    "@nl-portal/nl-portal-api": "https://nl-portal-frontend-snapshots.s3.eu-central-1.amazonaws.com/3.0.0-RC-202510290916-af39d4df-SNAPSHOT/nl-portal-nl-portal-api.tgz",
    "@nl-portal/nl-portal-authentication": "https://nl-portal-frontend-snapshots.s3.eu-central-1.amazonaws.com/3.0.0-RC-202510290916-af39d4df-SNAPSHOT/nl-portal-nl-portal-authentication.tgz",
    "@nl-portal/nl-portal-localization": "https://nl-portal-frontend-snapshots.s3.eu-central-1.amazonaws.com/3.0.0-RC-202510290916-af39d4df-SNAPSHOT/nl-portal-nl-portal-localization.tgz",
    "@nl-portal/nl-portal-user-interface": "https://nl-portal-frontend-snapshots.s3.eu-central-1.amazonaws.com/3.0.0-RC-202510290916-af39d4df-SNAPSHOT/nl-portal-nl-portal-user-interface.tgz",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-intl": "7.1.14",
    "react-router": "7.9.4"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "@vitejs/plugin-react": "4.7.0",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.4.0",
    "pwa-asset-generator": "8.1.2",
    "rimraf": "6.0.1",
    "typescript": "5.9.3",
    "vite": "7.1.11",
    "vitest": "3.2.4"
  }
}
