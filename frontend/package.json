{
  "name": "nl-portal-app",
  "version": "2.1.0",
  "author": "Ritense",
  "packageManager": "pnpm@10.17.1",
  "license": "EUPL-1.2",
  "type": "module",
  "engines": {
    "node": "20",
    "pnpm": "^10"
  },
  "scripts": {
    "dev": "vite --port 3000 --mode development",
    "build": "tsc && vite build",
    "preinstall": "npx only-allow pnpm",
    "preview": "vite preview --port 3000",
    "generate-pwa-assets": "pwa-asset-generator ./src/assets/pwa-logo.svg ./public/pwa --favicon --mstile --opaque  false --maskable true --index  index.html --manifest ./public/manifest.json --path-override '../pwa'",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@apollo/client": "3.10.4",
    "@gemeente-denhaag/design-tokens-components": "^5.1.0",
    "@gemeente-denhaag/icons": "^4.0.0",
    "@nl-portal/nl-portal-api": "https://nl-portal-frontend-snapshots.s3.eu-central-1.amazonaws.com/2.1.0-next-minor-202509151145-660924de-SNAPSHOT/nl-portal-nl-portal-api.tgz",
    "@nl-portal/nl-portal-authentication": "https://nl-portal-frontend-snapshots.s3.eu-central-1.amazonaws.com/2.1.0-next-minor-202509151145-660924de-SNAPSHOT/nl-portal-nl-portal-authentication.tgz",
    "@nl-portal/nl-portal-localization": "https://nl-portal-frontend-snapshots.s3.eu-central-1.amazonaws.com/2.1.0-next-minor-202509151145-660924de-SNAPSHOT/nl-portal-nl-portal-localization.tgz",
    "@nl-portal/nl-portal-user-interface": "https://nl-portal-frontend-snapshots.s3.eu-central-1.amazonaws.com/2.1.0-next-minor-202509151145-660924de-SNAPSHOT/nl-portal-nl-portal-user-interface.tgz",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-intl": "^7.1.11",
    "react-router": "^7.7.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "pwa-asset-generator": "^8.1.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
